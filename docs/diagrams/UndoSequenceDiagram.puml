@startuml
scale 1.5
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":WemeParser" as WemeParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedMemeBook" as VersionedMemeBook MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> WemeParser : parseCommand(undo)
activate WemeParser

create UndoCommand
WemeParser -> UndoCommand
activate UndoCommand

UndoCommand --> WemeParser
deactivate UndoCommand

WemeParser --> LogicManager : u
deactivate WemeParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoMemeBook()
activate Model

Model -> VersionedMemeBook : undo()
activate VersionedMemeBook

VersionedMemeBook -> VersionedMemeBook :resetData(ReadOnlyMemeBook)
VersionedMemeBook --> Model :
deactivate VersionedMemeBook

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
